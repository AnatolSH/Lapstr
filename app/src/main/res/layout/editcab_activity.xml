<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/editcab_activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditCabinet">

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/blog_list88"
        android:clipChildren="false"
        app:reverseLayout="false"
        android:paddingTop="5dp"
        android:paddingBottom="53dp"
        android:layout_above="@+id/imageButton3"
        android:layout_alignParentTop="true"
        android:clipToPadding="false">
    </android.support.v7.widget.RecyclerView>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:orientation="horizontal"
        android:id="@+id/nav_bar"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom">

        <ImageButton
            app:srcCompat="@mipmap/ic_home_white_36dp"
            android:id="@+id/imageButton2"
            android:layout_gravity="bottom"
            android:scaleType="fitCenter"
            android:background="?attr/colorPrimary"
            android:padding="7dp"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/imageButton3"
            android:layout_toStartOf="@+id/imageButton3" />

        <ImageButton
            android:layout_height="match_parent"
            app:srcCompat="@mipmap/ic_account_box_white_36dp"
            android:id="@+id/imageButton5"
            android:scaleType="fitCenter"
            android:background="@color/colorPrimary"
            android:layout_gravity="bottom"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/imageButton4"
            android:layout_toEndOf="@+id/imageButton4"
            android:padding="7dp"
            android:layout_width="75dp" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:srcCompat="@mipmap/ic_exit_to_app_white_36dp"
            android:id="@+id/imageButton6"
            android:scaleType="fitCenter"
            android:background="@color/colorPrimary"
            android:layout_toRightOf="@+id/imageButton5"
            android:layout_toEndOf="@+id/imageButton5"
            android:layout_gravity="bottom"
            android:padding="7dp" />

        <ImageButton
            app:srcCompat="@mipmap/ic_add_box_white_36dp"
            android:id="@+id/imageButton3"
            android:scaleType="fitCenter"
            android:background="@color/colorPrimary"
            android:layout_gravity="bottom"
            android:padding="7dp"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/imageButton4"
            android:layout_toStartOf="@+id/imageButton4"
            android:layout_width="75dp" />

        <ImageButton
            android:layout_width="75dp"
            android:layout_height="match_parent"
            app:srcCompat="@mipmap/ic_photo_camera_white_36dp"
            android:id="@+id/imageButton4"
            android:scaleType="fitCenter"
            android:background="@color/colorPrimary"
            android:layout_gravity="bottom"
            android:padding="7dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

    </RelativeLayout>


</android.support.design.widget.CoordinatorLayout>
